{
  "name": "js-exercises",
  "version": "2.0.0",
  "description": "app for training your js skills",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "gulp dev && node server/transformFilesToStrings.js .tmp",
    "devJs": "webpack-dev-server --open --config webpack.dev.js",
    "build": "gulp prod && node server/transformFilesToStrings.js dist",
    "buildJs": "webpack webpack --config webpack.prod.js",
    "clean": "gulp clean",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pure-js/js-exercises.git"
