{
  "name": "js-exercises",
  "version": "1.0.0",
  "description": "app for training your js skills",
  "main": "index.js",
  "scripts": {
    "start": "npm run less && npm run highlights && npm run pug && npm run jsmin && npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "less": "lessc -x src/index.less build/atom-dark-syntax.css",
    "serve": "browser-sync start --config 'bs-config.js'",
    "pug": "pug src/index.pug -o build",
    "jsmin": "uglifyjs src/main.js -o build/main.min.js",
    "highlights": "concurrently \"npm run highlights1\" \"npm run highlights2\" \"npm run highlights3\" ",
    "highlights1": "highlights exercises/hoisting/1.js -o build/1.html",
    "highlights2": "highlights exercises/hoisting/2.js -o build/2.html",
    "highlights3": "highlights exercises/hoisting/3.js -o build/3.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IamEmpty/js-exercises.git"
  },
  "author": "IamEmpty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IamEmpty/js-exercises/issues"
  },
  "homepage": "https://github.com/IamEmpty/js-exercises#readme",
  "dependencies": {
    "concurrently": "^2.2.0",
    "highlights": "^1.4.1"
  }
}
